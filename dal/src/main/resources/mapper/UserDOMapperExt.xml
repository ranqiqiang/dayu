<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caicai.ottx.dal.mapper.UserDOMapperExt">
  <!-- 扩展自动生成或自定义的SQl语句写在此文件中 -->

  <select id="queryUserByUsername" parameterType="String" resultMap="BaseResultMap">

    select * from USER
    WHERE username = #{username}

  </select>

  <select id="queryUserByUid" parameterType="long" resultMap="BaseResultMap">
    select * from USER
    WHERE id = #{userid}
    ;
  </select>



</mapper>
